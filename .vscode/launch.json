{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main Debug",
            "type": "php",
            "request": "launch",
            "runtimeArgs": [
                "-dxdebug.mode=debug",
                "-dxdebug.start_with_request=yes",
            ],
            "program": "",
            "cwd": "${workspaceRoot}",
            "port": 9003,
            "runtimeExecutable": "D:\\xampp\\php\\php.exe",
            "serverReadyAction": {
                "pattern": "Development Server \\(https://ug.local.com/\\) started",
                "uriFormat": "https://ug.local.com:%s",
                "action": "openExternally"
            }
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Other Debug",
            "url": "https://ug.local.com",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
